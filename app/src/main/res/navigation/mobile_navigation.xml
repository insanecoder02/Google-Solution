<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.google_solution.Fragment.Home"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
    </fragment>

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.example.google_solution.Fragment.Event"
        android:label="@string/title_event"
        tools:layout="@layout/fragment_event" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.google_solution.Fragment.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.google_solution.Fragment.Account"
        android:label="account"
        tools:layout="@layout/fragment_account"/>

    <fragment
        android:id="@+id/fabButton"
        android:name="com.example.google_solution.Fragment.AiBot"
        android:label="account"
        tools:layout="@layout/fragment_ai_bot"/>

</navigation>